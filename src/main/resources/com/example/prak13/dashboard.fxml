<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.prak13.DashboardController">
   <children>
      <TableView fx:id="table" layoutX="31.0" layoutY="43.0" prefHeight="261.0" prefWidth="537.0">
        <columns>
          <TableColumn fx:id="colUser" prefWidth="270.0" text="Username" />
          <TableColumn fx:id="colPass" prefWidth="270.0" text="Password" />
        </columns>
      </TableView>
      <TextField fx:id="user" layoutX="31.0" layoutY="318.0" promptText="username" />
      <TextField fx:id="pass" layoutX="201.0" layoutY="318.0" promptText="password" />
      <Button layoutX="377.0" layoutY="318.0" mnemonicParsing="false" onAction="#add" text="Add" />
      <Button layoutX="517.0" layoutY="318.0" mnemonicParsing="false" onAction="#delete" text="Delete" />
      <Button layoutX="439.0" layoutY="318.0" mnemonicParsing="false" onAction="#update" text="Update" />
   </children>
</AnchorPane>
